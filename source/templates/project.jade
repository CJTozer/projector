extend default

block content
  script(src='https://www.gstatic.com/charts/loader.js')

  h2.
    !{project}

  div.left
    div.content#time
      text TIME
  div.right#risks
    div.content#time
      text RISKS
  div.clear

  div
    div.content#gantt_div

    div.left
      div.content#time
        text NOTES
    div.right#risks
      div.content#time
        text ACTIONS
    div.clear

  script.
    var task_array = !{task_array};
  script(src="scripts/gantt.js")
